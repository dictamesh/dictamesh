{
  "name": "@dictamesh/sdk-core",
  "version": "0.1.0",
  "description": "Core SDK for DictaMesh - Framework-agnostic client for data mesh operations",
  "keywords": [
    "dictamesh",
    "sdk",
    "data-mesh",
    "graphql",
    "real-time",
    "typescript"
  ],
  "author": "Controle Digital Ltda",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/click2-run/dictamesh.git",
    "directory": "packages/sdk-core"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./adapters/graphql": {
      "import": "./dist/esm/adapters/graphql/index.js",
      "require": "./dist/cjs/adapters/graphql/index.js",
      "types": "./dist/types/adapters/graphql/index.d.ts"
    },
    "./adapters/rest": {
      "import": "./dist/esm/adapters/rest/index.js",
      "require": "./dist/cjs/adapters/rest/index.js",
      "types": "./dist/types/adapters/rest/index.d.ts"
    },
    "./connectors/http": {
      "import": "./dist/esm/connectors/http/index.js",
      "require": "./dist/cjs/connectors/http/index.js",
      "types": "./dist/types/connectors/http/index.d.ts"
    },
    "./connectors/websocket": {
      "import": "./dist/esm/connectors/websocket/index.js",
      "require": "./dist/cjs/connectors/websocket/index.js",
      "types": "./dist/types/connectors/websocket/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:esm && npm run build:cjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "dev": "tsc --watch --module esnext --outDir dist/esm",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {},
  "optionalDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false
}
